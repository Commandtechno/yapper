{
	"name": "yapper",
	"main": "dist/src/index.js",
	"type": "module",
	"packageManager": "pnpm@9.2.0",
	"scripts": {
		"build": "tsc && cross-env NODE_ENV=development node .",
		"start": "cross-env NODE_ENV=production node .",
		"translate": "tsx languages/utils/interface.ts && pnpm prettier typings/language.d.ts --write",
		"prepare": "husky && prisma migrate dev && pnpm run translate",
		"tunnel": "cloudflared tunnel run"
	},
	"lint-staged": {
		"**/*.{ts,json}": [
			"pnpm prettier --write"
		],
		"languages/{en-US.json,interface.ts}": [
			"pnpm translate",
			"git add typings/language.d.ts"
		]
	},
	"dependencies": {
		"@discordjs/core": "^1.2.0",
		"@discordjs/rest": "^2.3.0",
		"@discordjs/ws": "^1.1.1",
		"@hono/node-server": "^1.12.0",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-metrics-otlp-grpc": "^0.52.1",
		"@opentelemetry/exporter-trace-otlp-grpc": "^0.52.1",
		"@opentelemetry/resource-detector-docker": "^0.1.2",
		"@opentelemetry/resources": "^1.25.1",
		"@opentelemetry/sdk-metrics": "^1.25.1",
		"@opentelemetry/sdk-node": "^0.52.1",
		"@opentelemetry/semantic-conventions": "^1.25.1",
		"@prisma/client": "^5.17.0",
		"@prisma/instrumentation": "^5.17.0",
		"@sapphire/bitfield": "^1.2.2",
		"@sapphire/snowflake": "^3.5.3",
		"@sentry/node": "^8.20.0",
		"@sentry/opentelemetry": "^8.20.0",
		"@sentry/tracing": "^7.117.0",
		"bufferutil": "^4.0.8",
		"colorette": "^2.0.20",
		"dotenv-extended": "^2.9.0",
		"hono": "^4.5.3",
		"husky": "^9.1.4",
		"i18next-intervalplural-postprocessor": "^3.0.0",
		"i18next": "^23.12.2",
		"stripe": "^16.5.0",
		"utf-8-validate": "^6.0.4",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@sentry/types": "^8.20.0",
		"@types/node": "^22.0.0",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"cross-env": "^7.0.3",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-neon": "^0.1.62",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint": "^8.57.0",
		"lint-staged": "^15.2.7",
		"prettier": "^3.3.3",
		"prisma": "^5.17.0",
		"tsx": "^4.16.2",
		"typescript": "^5.5.4"
	},
	"pnpm": {
		"overrides": {
			"discord-api-types": "0.37.91",
			"@discordjs/ws": "dev"
		}
	}
}
