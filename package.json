{
	"name": "yapper",
	"main": "dist/src/index.js",
	"type": "module",
	"packageManager": "pnpm@8.15.1",
	"scripts": {
		"build": "tsc && cross-env NODE_ENV=development node .",
		"start": "cross-env NODE_ENV=production node .",
		"translate": "tsx languages/utils/interface.ts && pnpm prettier typings/language.d.ts --write",
		"prepare": "husky && prisma migrate dev && pnpm run translate",
		"tunnel": "cloudflared tunnel run"
	},
	"lint-staged": {
		"**/*.{ts,json}": [
			"pnpm prettier --write"
		],
		"languages/{en-US.json,interface.ts}": [
			"pnpm translate",
			"git add typings/language.d.ts"
		]
	},
	"dependencies": {
		"@discordjs/core": "^1.1.1",
		"@discordjs/rest": "^2.2.0",
		"@discordjs/ws": "^1.0.2",
		"@hono/node-server": "^1.11.1",
		"@prisma/client": "^5.13.0",
		"@sapphire/bitfield": "^1.2.2",
		"@sapphire/snowflake": "^3.5.3",
		"@sentry/node": "^7.112.2",
		"@sentry/tracing": "^7.112.2",
		"bufferutil": "^4.0.8",
		"colorette": "^2.0.20",
		"datadog-metrics": "^0.11.1",
		"dotenv-extended": "^2.9.0",
		"hono": "^4.2.9",
		"husky": "^9.0.11",
		"i18next": "^23.11.3",
		"i18next-intervalplural-postprocessor": "^3.0.0",
		"stripe": "^15.4.0",
		"utf-8-validate": "^6.0.3",
		"zlib-sync": "^0.1.9"
	},
	"devDependencies": {
		"@sentry/types": "^7.112.2",
		"@types/got": "^9.6.12",
		"@types/node": "18.18.0",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.57.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-neon": "^0.1.62",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prisma": "^5.13.0",
		"tsx": "^4.7.3",
		"typescript": "^5.4.5"
	},
	"pnpm": {
		"overrides": {
			"discord-api-types": "file:../discord-api-types"
		}
	},
	"trustedDependencies": [
		"discord-api-types"
	]
}
